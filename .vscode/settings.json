{
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 1000,
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		// ".vscode": true,
		//optional
		"LICENSE": true,
		"**/.embuild/": true,
		// "**/.cargo/": true,
		// "**/Cargo.lock": true,
		"**/mod.rs": true,
		// "out/": true,
		// "**/target/": true,
		"**/sdkconfig.defaults": true,
		"**/build.rs": true,
		"docs/book/**": true,
		"html/assets/**": true
	},
	"rust-analyzer.cargo.features": "all",
	// "rust-analyzer.cargo.features": [
	// 	"web_sys_unstable_apis"
	// ],
	"rust-analyzer.cargo.target": "wasm32-unknown-unknown",
	"search.exclude": {
		"Cargo.lock": true,
		"**/assets/*": true,
		"**.svg": true
	},
	"rust-analyzer.showUnlinkedFileNotification": false
}